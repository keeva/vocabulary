{
/*  必填字段*/
  "manifest_version": 2,
  "version": "1.0",
  "name": "Vocabulary",
  "description": "This extension get English dictionary definition from Vocabulary.com",
  "icons": {
    "128": "icon128.png",
    "64": "icon64.png",
    "32":"icon32.png"
  },
  "browser_action": {
    "default_icon": "icon32.png",
    "default_popup": "popup.html"
  },
  "content_scripts":  [{
    "matches": ["http://*/*","https://*/*"],
    "js": ["jquery.js", "content.js"],
    "css":["main.css"],
    "all_frames": true
  }],
  "permissions": [
    "http://*/*","https://*/*"
  ],
  "web_accessible_resources": ["logo.png"]
}